{
  "name": "mongo-playground",
  "displayName": "mongo-playground",
  "description": "MongoDb Aggrigation Playground In your Vscode",
  "version": "0.0.7",
  "publisher": "HumedEssie",
  "repository": {
    "url": "https://github.com/Humed-Muhammad/mongo-playground"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mongoPlayground",
        "title": "Mongo Playground"
      }
    ]
  },
  "scripts": {
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "start": "sirv public",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "monaco-editor": "^0.46.0",
    "mongodb": "^6.4.0",
    "sirv-cli": "^2.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "icons": {
    "dark": "./icon.png",
    "light": "./icon.png"
  },
  "icon": "./icon.png"
}
